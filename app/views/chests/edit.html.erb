<h1>Editing chest</h1>

<% semantic_form_for @chest, :html => {:multipart => true} do |f|%>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
 <%= f.input :photo, :as => :file %>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @chest %> |
<%= link_to 'Back', chests_path %>
