<h1>New Chest</h1>

  <% semantic_form_for @chest, :html => {:multipart => true} do |f|%>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <%= f.input :photo, :as => :file %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', chests_path %>
