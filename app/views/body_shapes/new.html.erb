<h1>New body_shape</h1>

 <% semantic_form_for @body_shape, :html => {:multipart => true} do |f|%>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
 <%= f.input :photo, :as => :file %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', body_shapes_path %>
